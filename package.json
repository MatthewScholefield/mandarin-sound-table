{
  "name": "tone-perfect-sound-table",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://matthewscholefield.github.io/mandarin-sound-table",
  "dependencies": {
    "@material-ui/core": "^3.9.0",
    "gh-pages": "^2.0.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "gh-pages-deploy": {
    "staticpath": "build",
    "prep": [
      "build"
    ],
    "commit": "Update site",
    "post": [
      "clean-source"
    ],
    "noprompt": false
  },
  "devDependencies": {}
}
